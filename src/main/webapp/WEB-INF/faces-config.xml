<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
               http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule> 
        
        <navigation-case>
            <from-outcome>addPessoa</from-outcome> 
            <to-view-id>/cadastra-pessoa.xhtml</to-view-id> 
        </navigation-case>
        
        <navigation-case>
            <from-outcome>updatePessoa</from-outcome> 
            <to-view-id>/altera-pessoa.xhtml</to-view-id> 
        </navigation-case>
      
        <navigation-case>
            <from-outcome>listaPessoas</from-outcome> 
            <to-view-id>/lista-pessoas.xhtml</to-view-id> 
        </navigation-case>
        
        <navigation-case>
            <from-outcome>addMarca</from-outcome> 
            <to-view-id>/cadastra-marca.xhtml</to-view-id> 
        </navigation-case>
        
        <navigation-case>
            <from-outcome>teste</from-outcome> 
            <to-view-id>/teste-mensagem.xhtml</to-view-id> 
        </navigation-case>
    
        <navigation-case>
            <from-outcome>addEndereco</from-outcome> 
            <to-view-id>/cadastra-endereco.xhtml</to-view-id> 
        </navigation-case>
        
    </navigation-rule>
   
</faces-config>
